class Solution {
  public:
    vector<int> lcmAndGcd(int a, int b) {
        // code here
        int mn=min(a,b),mx=max(a,b),lcm=mx,gcd;
        for(int i=mn;i>0;i--){
            if(a%i==0 && b%i==0){
                gcd=i;
                break;
            }
        }
            int i=2;
        while(lcm%mn!=0) lcm=mx*i++;
        return{lcm,gcd};
    }
};
